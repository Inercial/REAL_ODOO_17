<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="stock_warehouse_orderpoint_form">
        <field name="name">stock.warehouse.orderpoint.form</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.view_warehouse_orderpoint_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="parent_category_id" />
            </xpath>
        </field>
    </record>
    <record model="ir.ui.view" id="stock_warehouse_orderpoint_tree">
        <field name="name">stock.warehouse.orderpoint.tree</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.view_warehouse_orderpoint_tree_editable" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="parent_category_id" />
            </xpath>
            <xpath expr="//field[@name='product_min_qty']" position="before">
                <field name="qty_min" optional="show" />
            </xpath>
        </field>
    </record>
</odoo>
