<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="mrp_dump_truck_driver_real_tree" model="ir.ui.view">
        <field name="name">mrp.dump.truck.driver.real.tree</field>
        <field name="model">mrp.dump.truck.driver</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="action_mrp_dump_truck_driver" model="ir.actions.act_window">
        <field name="name">Dump Truck Drivers</field>
        <field name="res_model">mrp.dump.truck.driver</field>
        <field name="view_mode">tree</field>
    </record>


    <menuitem
        id="mrp_dump_truck_driver_menu"
        name="Dump Truck Drivers"
        parent="mrp.menu_mrp_bom"
        action="action_mrp_dump_truck_driver"
    />
</odoo>
