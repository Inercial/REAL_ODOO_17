<odoo>
    <record id="view_account_analytic_line_filter_inherit_real" model="ir.ui.view">
        <field name="name">account.analytic.line.select.inherit.account</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="analytic.view_account_analytic_line_filter" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='groupby']" position="inside">
                <filter string="Publicado" name="posted" domain="[('move_line_id.parent_state', '=', 'posted')]" />
            </xpath>
        </field>
    </record>
</odoo>
